FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      B;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform (0 0 0.6);

boundaryField
{

    INLET
    {
        type            fixedValue;
        value           uniform (0 0 0.6);
    }
    OUTLET
    {
        type            fixedValue;
        value           uniform (0 0 0.6);
    }
    BOTTOM
    {
        type            fixedValue;
        value           uniform (0 0 0.6);
    }
    TOP
    {
        type            fixedValue;
        value           uniform (0 0 0.6);
    }
    frontAndBackPlanes
    {
        type            empty;
    }
}

